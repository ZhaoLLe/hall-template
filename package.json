{
  "name": "international.hiwork",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode test",
    "build": "vue-tsc && vite build",
    "build-test": "vue-tsc && vite build --mode test",
    "build-qa": "vue-tsc && vite build --mode qa",
    "build-demos": "vue-tsc && vite build --mode demos",
    "build-prod": "vue-tsc && vite build --mode prod",
    "preview": "vite preview",
    "eslint": "eslint --ext .js,.vue,.ts,.tsx,.jsx --ignore-path .eslintignore --fix src",
    "prettier": "prettier --write .",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@vueuse/core": "^10.4.1",
    "@vueuse/integrations": "^10.5.0",
    "axios": "^1.5.1",
    "element-plus": "^2.3.14",
    "pinia": "^2.1.6",
    "qs": "^6.11.2",
    "screenfull": "^6.0.2",
    "universal-cookie": "^6.1.1",
    "unplugin-icons": "^0.17.0",
    "vite-svg-loader": "^4.0.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vue": "^3.3.4",
    "vue-i18n": "9",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@iconify-json/ep": "^1.1.12",
    "@types/qs": "^6.9.8",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "unocss": "^0.56.5",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.5",
    "vite-plugin-pages": "^0.31.0",
    "vue-tsc": "^1.8.5"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run prettier",
      "npm run eslint"
    ]
  }
}
